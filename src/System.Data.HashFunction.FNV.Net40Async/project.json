{
  "version": "2.0.0-*",

  "authors": [ "Data.HashFunction Developers" ],
  "copyright": "Copyright 2014",
  "description": "Data.HashFunction implementation of Fowler–Noll–Vo hash function (http://www.isthe.com/chongo/tech/comp/fnv/index.html) (http://en.wikipedia.org/wiki/Fowler%E2%80%93Noll%E2%80%93Vo_hash_function).",
  "title": "Data.HashFunction.FNV",

  "buildOptions": {
    "define": [
      "INCLUDE_ASYNC"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true,
    "compile": {
      "include": [
        "../System.Data.HashFunction.FNV/**/*.cs",
        "../SolutionInfo.cs"
      ],
      "exclude": [
        "../System.Data.HashFunction.FNV/obj/**/*.cs"
      ]
    }
  },

  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/brandondahler/Data.HashFunction/master/LICENSE",
    "releaseNotes": "https://github.com/brandondahler/Data.HashFunction/wiki/Release-Notes",
    "repository": {
      "type": "git",
      "url": "https://github.com/brandondahler/Data.HashFunction/"
    },
    "tags": [ "FNV", "hash", "function", "Fowler–Noll–Vo", "Fowler", "Noll", "Vo" ]
  },

  "dependencies": {
    "System.Data.HashFunction.Core.Net40Async": {
      "target": "project"
    }
  },

  "frameworks": {
    "net40": {
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.16"
      }
    }
  }
